
<h4>Let's updated your account.</h4>
<%= form_for @user, method: :put do |f| %>
<p>
	<%= f.label :username %>
	<%= f.text_field :username %>
</p>

<p>
	<%= f.label :email %>
	<%= f.text_field :email %>
</p>
<p>
	<%= f.label :password %>
	<%= f.password_field :password %>
</p>
<%= f.submit %>
<% end %>